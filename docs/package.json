{
  "name": "docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean:typedocs": "rimraf ./docs/api/",
    "docusaurus": "docusaurus",
    "start": "npm run clean:typedocs && docusaurus start",
    "build": "npm run clean:typedocs && npm run build:docker-jumpstart && docusaurus build",
    "build:only": "docusaurus build",
    "build:docker-jumpstart": "cpy ./docker-jumpstart ./build/ && cpy ../packages/backend/server/examples ./build/docker-jumpstart && node ./zip-docker.js && rimraf ./build/docker-jumpstart",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@docusaurus/core": "3.0.1",
    "@docusaurus/preset-classic": "3.0.1",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.0.0",
    "prism-react-renderer": "^2.3.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.0.1",
    "@docusaurus/tsconfig": "3.0.1",
    "@docusaurus/types": "3.0.1",
    "bestzip": "^2.2.1",
    "cpy-cli": "^5.0.0",
    "docusaurus-plugin-typedoc": "^0.21.0",
    "rimraf": "^5.0.5",
    "typedoc": "^0.25.4",
    "typedoc-plugin-markdown": "^3.17.1",
    "typescript": "~5.2.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}
