  },
  "dependencies": {
    "@dapperduckling/keycloak-connector-common": "file:../../common",
    "js-cookie": "^3.0.5",
    "typia": "^5.2.6"
  },
  "yalcWatch": {
    "watchFolder": "dist",
    "buildWatchCommand": "tsc --watch",
    "extensions": "js,ts"
  },
